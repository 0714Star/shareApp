 .ql-clipboard {
  position: fixed;
  top: -1000px;
  left: -1000px;
}
.uni-container {
  background: #f8f8f8;
  padding-bottom: 66px;
  margin-top: 54px;
}
.uni-container .header {
  width: 950px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  z-index: 999;
  height: 60px;
  display: flex;
  align-items: center;
}
.uni-container .header .back {
  cursor: pointer;
  color: #333;
}
.uni-container .footer {
  background: #fff;
  height: 56px;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: #ebebeb solid 1px;
  z-index: 101;
}
.uni-container .footer .wrap {
  width: 950px;
  margin: 0 auto;
  display: flex;
  align-items: center;
  height: 100%;
}
.uni-container .footer .wrap .left {
  flex: 1;
  display: flex;
  align-items: center;
  gap: 10px;
}
.uni-container .footer .wrap .left .word-count, .uni-container .footer .wrap .left .auto-save {
  font-size: 12px;
  color: #666;
}
.uni-container .footer .wrap .right {
  display: flex;
  align-items: center;
}
.uni-container .footer .wrap .right .uni-button-group {
  margin: 0;
}
.uni-container .edit-box {
  width: 950px;
  padding: 32px 64px;
  box-sizing: border-box;
  margin: 0 auto;
  background: #fff;
  border-radius: 2px;
}
.uni-container .title, .uni-container .editor {
  border-bottom: #f2f2f2 solid 1px;
  padding-bottom: 24px;
}
.uni-container .editor {
  padding-top: 24px;
}
.uni-container .title .uni-input {
  font-size: 23px;
  font-weight: bold;
  width: 100%;
  min-height: 0;
}
.uni-container .settings {
  margin-top: 24px;
}
.uni-container .settings .excerpt {
  border-radius: 2px;
  border: #e5e5e5 solid 1px;
  width: 100%;
  padding: 10px;
  font-size: 14px;
}
.uni-container .thumbnail-list {
  display: flex;
  gap: 10px;
  margin-top: 20px;
}
.uni-container .thumbnail {
  width: 150px;
  height: 150px;
  border: #e5e5e5 solid 1px;
  border-radius: 2px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  cursor: pointer;
  overflow: hidden;
}
.uni-container .thumbnail .placeholder {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.uni-container .thumbnail .remove-thumbnail {
  position: absolute;
  top: 10px;
  right: 10px;
  background: rgba(0, 0, 0, 0.5);
  padding: 3px;
  border-radius: 50%;
  width: 20px;
  height: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 2;
}
.uni-container .thumb-mode .mode {
  margin-right: 10px;
}
.uni-container .thumb-mode .mode:last-child {
  margin-right: 0;
}
.uni-container .thumb-mode .mode .uni-radio-input {
  transform: scale(0.7);
}
@media (max-width: 768px) {
.uni-container {
    margin-top: 0;
    padding: 0;
}
.uni-container .header {
    display: none;
}
.uni-container .title, .uni-container .editor {
    border-bottom: #f2f2f2 solid 1px;
    padding-bottom: 12px;
}
.uni-container .edit-box {
    width: 100%;
    padding: 10px;
}
.uni-container .settings .excerpt {
    width: auto;
}
.uni-container .footer {
    display: none;
}
}
@media (min-width: 768px) {
.uni-container .uni-button-group.m {
    display: none;
}
}